{
    "app": {
        "title": "Test Title",
        "version": "12.0.0",
        "auto_start_monitoring": false,
        "check_for_updates": true,
        "language": "ru",
        "theme": "system",
        "backup_dir": "backups",
        "max_backups": 10,
        "last_backup": ""
    },
    "monitoring": {
        "enabled": true,
        "check_interval": 300,
        "parallel_checks": 5,
        "retry_count": 3,
        "retry_delay": 60,
        "use_browser": true,
        "timeout": 30,
        "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
        "browser_wait": 5,
        "browser_args": [
            "--headless",
            "--disable-gpu",
            "--no-sandbox",
            "--disable-dev-shm-usage"
        ]
    },
    "database": {
        "path": "data/wdm_database.db",
        "backup_on_start": true,
        "backup_on_exit": true,
        "auto_vacuum": true
    },
    "notifications": {
        "enabled": true,
        "desktop_notifications": true,
        "email_notifications": false,
        "email_settings": {
            "smtp_server": "",
            "smtp_port": 587,
            "smtp_username": "",
            "smtp_password": "",
            "from_address": "",
            "to_address": ""
        },
        "notification_threshold": 5.0,
        "cooldown_period": 3600
    },
    "proxy": {
        "enabled": false,
        "type": "http",
        "host": "",
        "port": 8080,
        "username": "",
        "password": ""
    },
    "logging": {
        "level": "INFO",
        "max_file_size": 10485760,
        "max_files": 5,
        "log_to_console": true
    }
}